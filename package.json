  "scripts": {
    "dev": "ts-node src/_local/run.ts",
    "lint": "eslint --fix . && prettier --write .",
    "lint:nofix": "eslint . && prettier --check .",
    "test": "jest --passWithNoTests",
    "test+": "pnpm run lint && pnpm run test",
    "clean": "shx rm -rf ./dist",
